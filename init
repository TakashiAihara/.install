mkdir plugins

git submodule add --force https://github.com/anishathalye/dotbot plugins/dotbot
git submodule add --force https://github.com/sobolevn/dotbot-brewfile.git plugins/dotbot-brewfile
git submodule add --force https://github.com/bryant1410/dotbot-apt plugins/dotbot-apt
git submodule add --force https://github.com/delicb/dotbot-golang plugins/dotbot-golang

git config --file=.gitmodules submodule.plugins/dotbot.ignore dirty
git config --file=.gitmodules submodule.plugins/dotbot-brewfile.ignore dirty
git config --file=.gitmodules submodule.plugins/dotbot-apt.ignore dirty
git config --file=.gitmodules submodule.plugins/dotbot-golang.ignore dirty

git submodule add --force https://github.com/TakashiAihara/.dotfiles links/.dotfiles
git submodule add --force https://github.com/TakashiAihara/.ssh links/.ssh
git submodule add --force https://github.com/TakashiAihara/etc links/etc
git submodule add --force https://github.com/TakashiAihara/rclone links/rclone
